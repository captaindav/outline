{
  "name": "vue-outline",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build": "node scripts/build.js",
    "clean": "lerna clean",
    "init:drupal": "cp -R scripts/drupal packages/drupal && cd packages/drupal && composer install && lando poweroff && lando start && lando site-setup",
    "lint": "lerna run lint --parallel --stream",
    "lint:fix": "lerna run lint:fix --parallel",
    "postinstall": "lerna bootstrap",
    "serve": "node scripts/serve.js",
    "start": "cd packages/drupal && lando start"
  },
  "engines": {
    "node": ">= 12"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "cross-spawn": "^6.0.5",
    "eslint": "^7.14.0",
    "lerna": "^3.22.1",
    "lodash": "^4.17.20"
  }
}
